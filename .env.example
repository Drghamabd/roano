APP_NAME=Laravel
APP_ENV=production  # تم تغييرها إلى production
APP_KEY=base64:your_generated_app_key_here # يجب استبدال هذا بمفتاح التطبيق الفعلي الخاص بك
APP_DEBUG=false     # تم تغييرها إلى false
APP_URL= # لا حاجة لتعيين هذا هنا، Railway سيوفرها تلقائياً

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# متغيرات قاعدة البيانات: تم تعليقها أو إزالتها تماماً
# Railway سيوفر هذه المتغيرات من خلال لوحة التحكم الخاصة به
# DB_CONNECTION=mysql
# DB_HOST=
# DB_PORT=
# DB_DATABASE=
# DB_USERNAME=
# DB_PASSWORD=

SESSION_DRIVER=file # يُفضل 'file' أو 'cookie' على 'database' في هذه البيئات
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync # يُفضل 'sync' لتجنب مشاكل إعداد طابور الرسائل المعقدة في البداية

CACHE_STORE=file # يُفضل 'file' لتجنب مشاكل إعداد الكاش في البداية
# CACHE_PREFIX=

# متغيرات Memcached: تم تعليقها إذا لم تستخدم Memcached
# MEMCACHED_HOST=127.0.0.1

# متغيرات Redis: تم تعليقها إذا لم تستخدم Redis
# REDIS_CLIENT=phpredis
# REDIS_HOST=127.0.0.1
# REDIS_PASSWORD=null
# REDIS_PORT=6379

MAIL_MAILER=log # يُفضل 'log' لتجنب مشاكل إعداد البريد الإلكتروني في البداية
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
